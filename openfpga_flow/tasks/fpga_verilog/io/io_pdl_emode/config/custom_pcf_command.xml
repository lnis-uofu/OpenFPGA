<pcf_config>
  <command name="set_delay_chain" type="delay_chain">
    <!-- Define an option -pad where user must provide a valid pin name available in pin table CSV -->
    <option name="pad" type="pin">
    <option name="delay" type="mode">
      <!-- Assume min. step is 0.1ns and max range is 0.3ns -->
      <mode name="0.0ns">000</mode>
      <mode name="0.1ns">001</mode>
      <mode name="0.2ns">010</mode>
      <mode name="0.3ns">011</mode>
      <mode name="0.4ns">100</mode>
      <mode name="0.5ns">101</mode>
      <mode name="0.6ns">110</mode>
      <mode name="0.7ns">111</mode>
    </option>
  </command>
    <command name="set_watch_dog" type="delay_chain">
    <option name="pad" type="pin">
    <option name="mode" type="mode">
      <!-- Assume three modes: NORMAL, LOW_SPEED, HIGH_SPEED -->
      <mode name="NORMAL">0001</mode>
      <mode name="LOW_SPEED">0010</mode>
      <mode name="HIGH_SPEED">0100</mode>
    </option>
  </command>
</pcf_config>
